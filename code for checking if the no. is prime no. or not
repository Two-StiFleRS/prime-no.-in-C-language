#include <stdio.h>
#include<stdlib.h>
int main()
{
    // to find if a no. is a prime no. or not
    int i=2,a;
    printf("enter the number\n");
    scanf("%d",&a);
    while(i<a)
    {
        if(a%i==0)
        {
            printf("%d is not a prime no.",a);
            goto hpp;
        }

        i=i+1;

    }
printf("%d is a prime no.",a);
hpp:
    return 0;
}
